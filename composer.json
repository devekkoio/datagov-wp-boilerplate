    "wpackagist-plugin/w3-total-cache": "0.9.4.1",
    "wpackagist-plugin/wordpress-importer": "0.6.3",
    "wpackagist-plugin/wp-crontrol": "~1.3",
    "gsa/arcgis-map": "dev-master",
    "gsa/bwp-external-links": "dev-master",
    "gsa/custom-permalinks": "dev-master",
    "gsa/custom-post-view-generator": "dev-master",
    "gsa/data.gov": "dev-master",
    "gsa/datagov-custom": "dev-master",
    "gsa/interactive-world-maps": "dev-master",
    "gsa/metric-count": "dev-master",
    "gsa/q-and-a": "dev-master",
    "gsa/restrict-categories": "dev-master",
    "gsa/saml-20-single-sign-on": "dev-max",
    "gsa/sticky-posts-in-category": "dev-master",
    "gsa/twitter-posts-to-blog": "dev-master",
    "gsa/twitter-widget-pro": "dev-master",
    "gsa/usa-search": "dev-master",
    "gsa/wordpress-seo": "dev-master",
    "gsa/wp-rss-multi-importer": "dev-master",
    "gsa/wp-sitemap-page": "dev-master",
    "gsa/wp-open311": "dev-master",
    "philipashlock/wp-no-taxonomy-base": "dev-master"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.5.1",
    "wpackagist-plugin/check-email": "*",
    "wpackagist-plugin/debug-bar": "*",
    "wpackagist-plugin/log-deprecated-notices": "*",
    "wpackagist-plugin/query-monitor": "*"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/data.gov",
      "no-api": "true"
    },
    {
      "type": "package",
      "package": {
        "name": "philipashlock/wp-no-taxonomy-base",
        "version": "dev-master",
        "type": "wordpress-plugin",
        "source": {
          "url": "git@github.com:philipashlock/wp-no-taxonomy-base.git",
          "type": "git",
          "reference": "master"
        }
      }
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/arcgis-map",
      "no-api": "true"
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/bwp-external-links",
      "no-api": "true"
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/custom-permalinks",
      "no-api": "true"
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/custom-post-view-generator",
      "no-api": "true"
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/datagov-custom",
      "no-api": "true"
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/interactive-world-maps",
      "no-api": "true"
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/metric-count",
      "no-api": "true"
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/q-and-a",
      "no-api": "true"
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/restrict-categories",
      "no-api": "true"
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/saml-20-single-sign-on",
      "no-api": "true"
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/sticky-posts-in-category",
      "no-api": "true"
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/twitter-posts-to-blog",
      "no-api": "true"
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/twitter-widget-pro",
      "no-api": "true"
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/usa-search",
      "no-api": "true"
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/wordpress-seo",
      "no-api": "true"
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/wp-rss-multi-importer",
      "no-api": "true"
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/wp-sitemap-page",
      "no-api": "true"
    },
    {
      "type": "git",
      "url": "git@github.com:GSA/wp-open311",
      "no-api": "true"
    }
  ],
  "minimum-stability": "stable",
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "web/app/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "web/app/themes/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "web/wp"
  },
  "config": {
    "preferred-install": "dist"
  }
